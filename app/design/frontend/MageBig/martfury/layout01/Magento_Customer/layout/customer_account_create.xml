<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
		<referenceContainer name="page.wrapper">
			<block class="Magento\Cms\Block\Block" name="Register_Banner" before="-">
				<arguments>
					<argument name="block_id" xsi:type="string">register_page_banner</argument>
				</arguments>
			</block>
		</referenceContainer>
		<move element="Register_Banner" destination="main.content" before="-"/>
    </body>
</page>
